{
  "crons": [
    {
      "path": "/api/payload-jobs/run",
      "schedule": "0 0 * * *"
    }
  ],
  "functions": {
    "app/(frontend)/[locale]/goods/page.tsx": {
      "maxDuration": 30
    },
    "app/(frontend)/[locale]/open-positions/[slug]/page.tsx": {
      "maxDuration": 30
    },
    "app/(frontend)/[locale]/posts/[slug]/page.tsx": {
      "maxDuration": 30
    },
    "app/(frontend)/[locale]/products/[slug]/page.tsx": {
      "maxDuration": 30
    },
    "app/(frontend)/[locale]/layout.tsx": {
      "maxDuration": 30
    },
    "app/sitemap.ts": {
      "maxDuration": 30
    },
    "app/api/**": {
      "maxDuration": 30
    }
  },
  "buildCommand": "pnpm run build",
  "installCommand": "pnpm install",
  "regions": ["iad1"],
  "framework": "nextjs"
}
